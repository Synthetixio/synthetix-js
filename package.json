{
  "name": "synthetix-js",
  "version": "2.66.0",
  "description": "Synthetix JS Library - access Synthetix smart contracts from browser and Node.js",
  "main": "dist/main.node.js",
  "browser": "dist/main.js",
  "homepage": "https://developer.synthetix.io",
  "repository": "https://github.com/Synthetixio/synthetix-js",
  "scripts": {
    "test": "jest --runInBand --verbose",
    "clean-install": "rm ./package-lock.json && rm -rf node_modules/* && npm install",
    "clear": "$(rm dist/*.js || true) && $(rm src/contracts/*.js || true) && $(rm lib/abis/*.js || true) && $(rm lib/addresses.js || true)",
    "lint": "eslint .",
    "format": "prettier --write '{tools,lib,src,__tests__}/**/*.js'",
    "abitojs": "node ./tools/abitojs.js",
    "webpack": "webpack --mode=production",
    "build": "npm run clear && npm run abitojs && npm run format && npm run webpack && npm test"
  },
  "author": "Synthetix",
  "license": "MIT",
  "dependencies": {
    "@ledgerhq/hw-app-eth": "4.74.2",
    "@ledgerhq/hw-transport": "4.74.2",
    "@ledgerhq/hw-transport-u2f": "4.74.2",
    "@portis/web3": "^2.0.0-beta.59",
    "@walletconnect/web3-provider": "^1.0.15",
    "ethereumjs-tx": "1.3.7",
    "ethereumjs-util": "5.2.0",
    "ethers": "4.0.44",
    "hdkey": "1.1.1",
    "lodash": "4.17.15",
    "synthetix": "2.66.0",
    "trezor-connect": "8.1.8",
    "walletlink": "2.0.2"
  },
  "devDependencies": {
    "@babel/core": "~7.0.0",
    "@babel/plugin-transform-runtime": "~7.0.0",
    "@babel/preset-env": "~7.0.0",
    "@babel/runtime": "~7.0.0",
    "babel-core": "~7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "24.8.0",
    "babel-loader": "~8.0.2",
    "eslint": "^5.16.0",
    "eslint-config-last": "0.0.5",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "24.8.0",
    "prettier": "~1.14.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2"
  }
}
