{
  "name": "synthetix-js",
  "version": "2.4.2-alpha",
  "description": "Synthetix JS Library - access Synthetix smart contracts from browser and Node.js",
  "main": "dist/main.node.js",
  "browser": "dist/main.js",
  "homepage": "https://developer.synthetix.io",
  "repository": "https://github.com/Synthetixio/synthetix-js",
  "scripts": {
    "test": "jest --runInBand",
    "clear": "$(rm dist/*.js || true) && $(rm src/contracts/*.js || true) && $(rm lib/abis/*.js || true) && $(rm lib/addresses.js || true)",
    "lint": "eslint .",
    "docs": "jsdoc src/index*.js src/contracts/mainnet src/util -r -d ./doc -c jsdoc.json --readme Readme.md",
    "format": "prettier --write '{tools,lib,src,__tests__}/**/*.js'",
    "abitojs": "node ./tools/abitojs.js",
    "webpack": "webpack --mode=production",
    "build": "npm run clear && npm run abitojs && npm run format && npm run docs && npm run webpack && npm run test"
  },
  "author": "Synthetix",
  "license": "MIT",
  "dependencies": {
    "@ledgerhq/hw-app-eth": "~4.24.0",
    "@ledgerhq/hw-transport": "~4.24.0",
    "@ledgerhq/hw-transport-u2f": "~4.28.0",
    "babel-eslint": "^10.0.1",
    "ethereumjs-tx": "~1.3.7",
    "ethereumjs-util": "~5.2.0",
    "ethers": "4.0.27",
    "lodash": "~4.17.11",
    "synthetix": "^2.4.2-alpha",
    "trezor-connect": "^7.0.1"
  },
  "devDependencies": {
    "@babel/core": "~7.0.0",
    "@babel/plugin-transform-runtime": "~7.0.0",
    "@babel/preset-env": "~7.0.0",
    "@babel/runtime": "~7.0.0",
    "axios": "~0.18.0",
    "babel-core": "~7.0.0-bridge.0",
    "babel-loader": "~8.0.2",
    "cheerio": "~1.0.0-rc.2",
    "docdash": "~1.0.0",
    "eslint": "^5.16.0",
    "eslint-config-last": "0.0.5",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "~23.5.0",
    "jsdoc": "~3.5.5",
    "prettier": "~1.14.2",
    "pretty-quick": "~1.6.0",
    "rollup": "~0.65.0",
    "rollup-plugin-babel": "~4.0.3",
    "webpack": "~4.17.2",
    "webpack-cli": "~3.1.0"
  }
}
